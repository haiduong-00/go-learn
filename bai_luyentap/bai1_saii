package main

import (
	"fmt"
)

func findNthTerm(N int) int {
	x := []int{1,3,6,10,15,21}
	for i, v := range x {
		if i == 1 {
			fmt.Println(v)
		} else if i == 2 {
			fmt.Println(v)
		} else if i == 3 {
			fmt.Println(v)
		} else if i == 4 {
			fmt.Println(v)
		} else if i == 5 {
			fmt.Println(v)
		} else if i == 6 {
			fmt.Println(v)
		}
	}
	return N
}
func main() {

	fmt.Println()
}
