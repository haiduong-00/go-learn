package main

import "fmt"

func EvenOddSum(N int, Arr []int) (int, int) {
	even, odd := 0, 0
	for i := 1; i <= N; i++ {
		if i%2 == 0 {
			odd += Arr[i-1]
		} else {
			even += Arr[i-1]
		}
	}
	return odd, even
}

//Khi mà viết đến function, tự hình dung được code thực hiện chức năng gì

func main() {
	//a:=[5]int{1,2,3,4,5}
	fmt.Println(0)  // Integers
	// Unsigned: 0, 1, 2
	// Signed: -1, 0, 1

}